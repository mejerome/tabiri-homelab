# Example Terraform variables for NetBird VM deployment
# Copy this file to terraform.tfvars and update with your values

aws_region         = "us-east-1"
ssh_key_name       = "your-ssh-key-name"
admin_cidr_blocks  = ["YOUR_ADMIN_IP/32"]  # Replace with your admin IP
route53_zone_id    = "Z0YOURZONEID"
domain_name        = "netbird.yourdomain.com"
instance_type      = "t3.small"
volume_size        = 60
volume_encrypted   = true
